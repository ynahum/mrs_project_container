<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <node pkg="autodrive_f1tenth" exec="autodrive_bridge" name="autodrive_bridge" output="screen">
        <param name="emulate_tty" type="bool" value="true"/>
    </node>
    <arg name="rviz_config" default="$(find-pkg-share autodrive_f1tenth)/rviz/simulator.rviz"/>
    <node pkg="rviz2" exec="rviz2" name="rviz" output="screen" args="-d $(var rviz_config)" />
</launch>

