<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- <arg name="params_file" default="$(find-pkg-share my_car)/config/secondary/nav2_params_amcl.yaml"/> -->
    <!-- <arg name="params_file" default="$(find-pkg-share my_car)/config/nav2_params.yaml"/> -->
    <!-- <arg name="params_file" default="$(find-pkg-share my_car)/config/planner_controller_nav2_params.yaml"/> -->
    <arg name="params_file" default="$(find-pkg-share my_car)/config/minimal_nav2_params.yaml"/>
    <arg name="map" default="$(find-pkg-share my_car)/maps/practice_2_w_as_o.yaml"/>
    <include file="$(find-pkg-share my_car)/launch/my_nav2_bringup_launch.py">
        <arg name="params_file" value="$(var params_file)"/>
        <arg name="map" value="$(var map)"/>
    </include>

    <node pkg="my_car" exec="velocity_control_node" name="velocity_control_node" output="screen">
        <param name="kP_throttle" value="0.04"/>
        <param name="kP_steering" value="1.0"/>
    </node>

    <node pkg="my_car" exec="path_publisher" name="path_publisher_node" output="screen" >
        <param name="csv_path" value="/home/dev/ros_ws/src/my_car/maps/practice_2_w_as_o_centerline_manual.csv"/>
        <param name="lookahead" value="175"/>
    </node>
</launch>

