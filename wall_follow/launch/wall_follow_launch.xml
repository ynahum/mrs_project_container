<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- Launch the wall_follow_node with parameters -->
    <node pkg="wall_follow" exec="wall_follow_node" output="screen">
        <param name="kp_speed" value="0.04"/>
        <param name="kd_speed" value="0.0"/>
        <param name="ref_speed" value="1.8"/>
    </node>
    <!-- Include the launch file from another package -->
    <include file="$(find-pkg-share autodrive_f1tenth)/launch/simulator_bringup_rviz.launch.py" />
    <node
        pkg="slam_toolbox"
        exec="async_slam_toolbox_node"
        name="slam_toolbox"
        output="screen">
        <param from="/home/dev/ros_ws/src/config/mapper_params_online_async.yaml" />
    </node>
</launch>

